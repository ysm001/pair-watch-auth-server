#!/usr/bin/env node

var config = require('../config/db.json')['development'];
var db = config['host'] + '/' + config['database']
var query = ['mongo', db, 'config/seed.js'].join(' '); 

var exec = require('child_process').exec

console.log(query);
exec(query, function(err, stdout, stderr){
  if (err) throw err;
});
